<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RectangleImageView">
        <attr name="widthRatio" format="integer"/>
        <attr name="heightRatio" format="integer"/>
        <attr name="fixedDimension" format="enum">
            <enum name="width" value="0"/>
            <enum name="height" value="1"/>
        </attr>
    </declare-styleable>
    <string-array name="order_values_array">
        <item>popular</item>
        <item>top_rated</item>
    </string-array>
    <string-array name="order_keys_array">
        <item>Popular</item>
        <item>Top Rated</item>
    </string-array>
</resources>